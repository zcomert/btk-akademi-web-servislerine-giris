@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Countries</h1>
    <b>@Model.Countries.Count</b>
</div>
<div>
<ul class="list-group">

    @foreach (var item in @Model.Countries)
    {
        <li class="list-group-item">[@item.sISOCode] - @item.sName </li>
    }
</ul>

</div>